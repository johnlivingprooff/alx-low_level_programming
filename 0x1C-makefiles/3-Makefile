CC = gcc
SRC = main.c school.c
OBJ = main.o school.o
NAME = school
RM = rm -f

all: main.c school.c 
	$(CC) $(SRC) -o ${NAME}

$(NAME): main.o school.o
	$(CC) $(OBJ) -o $@

main.o: main.c
	$(CC) -c $< -o $@

school.o: school.c
	$(CC) -c $< -o $@

clean:
	$(RM) *~ school

oclean:
	$(RM) $(OBJ)

fclean: clean oclean

re: fclean all